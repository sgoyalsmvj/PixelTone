{
  "name": "@ai-studio/database",
  "version": "1.0.0",
  "description": "Database schema and ORM for AI Creative Studio",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/seed.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "prisma": "^5.6.0",
    "tsx": "^4.1.0",
    "vitest": "^0.34.6",
    "@types/node": "^20.8.0",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist",
    "prisma"
  ]
}